@model List<EmployeeManagement.Contracts.Employee>;
@{
    ViewBag.Title = "Employees";
}

@foreach (var emp in Model)
{
    @Html.ActionLink(linkText : emp.FirstName + " " + emp.LastName, 
                    actionName: "Details", 
                    controllerName: "Employee",
                    routeValues: new{ Id = emp.Id}, 
                    htmlAttributes: null);
    <br \>
}
